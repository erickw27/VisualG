algoritmo "semnome"
// Função :
// Autor :
// Data : 27/08/2020
// Seção de Declarações 
var S,P,N,MD:inteiro
   S <- 0
   P <- 0
  Para i <- 1 Até 10 faça
inicio
  Escreva ("Digite um número")
  Leia Ñ;
  Se N >= 0 Entao
  inicio
        S <- S+N
        P <- P+1
        Fim Se
     Fim Para
  MD <- S/P
Escreva ("A média é;",MD)
  
fimalgoritmo
